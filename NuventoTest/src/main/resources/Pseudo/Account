Object Account

    int ownerID     = -1
    int accNumber   = -1
    int balance     = -1

    function Account(newOwnerID, newAccNumber, newBalance)
        ownerID = newOwnerID
        accNumber = newAccNumber
        balance = newBalance

    int function getAccountOwnerID
        return ownerID

    int function getAccountNumber
        return number

    int function getBalance
        return balance

    void function setBalance: Pass newBalance
        balance = newBalance





